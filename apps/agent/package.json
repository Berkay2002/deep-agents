{
  "name": "deep-agents-app",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js"
    },
    "./deep-agent": {
      "types": "./dist/deep-agent/index.d.ts",
      "import": "./dist/deep-agent/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:examples": "yarn clean && tsc -p examples.tsconfig.json",
    "build:watch": "tsc --watch",
    "build:server": "langgraphjs build",
    "clean": "rm -rf dist dist-examples || true",
    "dev": "langgraphjs dev -c langgraph.json",
    "dev:server": "langgraphjs dev -c langgraph.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "ultracite check",
    "format": "ultracite fix",
    "prepublishOnly": "yarn build",
    "test": "echo \"Error: no test specified\" && exit 0",
    "start": "node dist/agent.js"
  },
  "dependencies": {
    "@langchain/core": "^0.3.78",
    "@langchain/google-genai": "^0.2.18",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/langgraph-sdk": "^0.1.0",
    "@langchain/mcp-adapters": "^0.6.0",
    "@langchain/openai": "^0.6.14",
    "@langchain/tavily": "^0.1.5",
    "exa-js": "^1.9.3",
    "jose": "^5.9.3",
    "langchain": "^0.3.35",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@langchain/langgraph-cli": "^0.0.69",
    "@tsconfig/recommended": "^1.0.10",
    "@types/node": "^24.6.1",
    "dotenv": "^17.2.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "ultracite": "^5.5.0"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist/**/*"
  ],
  "packageManager": "yarn@1.22.22"
}
